{
  "name": "im-support-bot",
  "version": "1.0.0",
  "author": "Soni",
  "description": "Discord bot for the TempleOS help desk",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soni801/temple-bot.git"
  },
  "homepage": "https://github.com/soni801/temple-bot#readme",
  "keywords": [
    "discord",
    "bot"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/soni801/temple-bot/issues"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env-shell NODE_ENV=development node --trace-warnings --inspect --require ts-node/register src/main.ts src/main.ts",
    "dev:watch": "cross-env-shell NODE_ENV=development nodemon --exec node --trace-warnings --inspect --require ts-node/register src/main.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "yarn run build:clean && yarn run build:src",
    "build:src": "cross-env-shell NODE_ENV=production tsc",
    "build:watch": "cross-env-shell NODE_ENV=production  tsc --watch",
    "build:clean": "rm -rf dist",
    "start": "cross-env-shell NODE_ENV=production node dist/main.js"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "axios": "^0.26.0",
    "cross-env": "^7.0.3",
    "discord.js": "^13.1.0",
    "dotenv": "^16.0.0",
    "turndown": "^7.1.1",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@discordjs/rest": "^0.3.0",
    "@types/node": "^16.11.0",
    "@types/turndown": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "discord-api-types": "^0.27.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
}
